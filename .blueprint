models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, two_factor_secret: 'text nullable', two_factor_recovery_codes: 'text nullable', two_factor_confirmed_at: 'datetime nullable', is_active: 'boolean default:1', remember_token: 'string:100 nullable', current_team_id: 'biginteger unsigned nullable', profile_photo_path: 'string:2048 nullable' }
    backend\Categoria: { nombre: 'string:45 nullable default:null unique', slug: "string:45 nullable default:null charset:'utf8'", softdeletes: softDeletes, relationships: { morphedByMany: 'Post, Video, Imagen' } }
    backend\Perfil: { id: false, user_id: 'id foreign:users.id primary', edad: unsignedTinyInteger, profesion: 'string:30 nullable default:null comment:"despues cambiar a tabla" charset:''utf8''', biografia: "longtext default:null charset:'utf8'", website: "string:45 default:null charset:'utf8'", softdeletes: softDeletes, relationships: { hasOne: User } }
    backend\UserSetting: { id: false, user_id: 'id foreign:users.id', theme: "string:20 default:'dark' charset:'utf8'", language: "string:5 default:'fr-FR' charset:'utf8'", autologin: 'boolean default:true', softdeletes: softDeletes, relationships: { hasOne: User } }
    backend\Marcador: { nombre: "string:45 nullable default:null unique charset:'utf8'", slug: "string:45 nullable default:null charset:'utf8'", hexa: "string:7 unique default:'#' charset:'utf8'", imagen: "string:128 nullable default:null charset:'utf8'", rgb: "string:20 nullable default:null charset:'utf8'", metadata: 'json nullable', softdeletes: softDeletes, relationships: { morphedByMany: 'Post:postIndex, Video:videoIndex, Imagen:imagenIndex' } }
    posts\Post: { user_id: 'id foreign:users.id', name: "string:32 charset:'utf8'", contenido: "longtext nullable charset:'utf8'", publicado: 'timestamp nullable useCurrent', actualizado: 'timestamp nullable useCurrent useCurrentOnUpdate', softdeletes: softDeletes, relationships: { morphToMany: 'Marcador:marcadorable, Categoria:categoriable' } }
    posts\Video: { name: "string:32 charset:'utf8'", user_id: 'id foreign:users.id', url: "string:128 charset:'utf8'", softdeletes: softDeletes, relationships: { morphToMany: 'Marcador:marcadorable, Categoria:categoriable' } }
    posts\Imagen: { name: "string:32 charset:'utf8'", user_id: 'id foreign:users.id', url: "string:128 charset:'utf8'", softdeletes: softDeletes, relationships: { morphToMany: 'Marcador:marcadorable, Categoria:categoriable' } }
